{
    "Logging": {
        "LogLevel": {
            "Default": "Debug",
            "System": "Information",
            "Microsoft": "Information"
        }
    },
    "SqlServer": {
      "ConnectionString": "server=(local);Initial Catalog=Dicom;Integrated Security=true;TrustServerCertificate=true",
      "Initialize": "true",
      "AllowDatabaseCreation": "true",
      "TransientFaultRetryPolicy": {
        "InitialDelay": "00:00:00.100",
        "RetryCount": 3,
        "Factor": 2,
        "FastFirst": true
      },
      "SchemaOptions": {
        "AutomaticUpdatesEnabled": true
      }
    }
}
