{
  "version": "2.0",
  "logging": {
    "applicationInsights": {
      "samplingSettings": {
        "isEnabled": true,
        "excludedTypes": "Request"
      }
    }
  },
  "DicomFunctions": {
    "Features": {
      "EnableExtendedQueryTags": true
    },
    "Services": {
      "ExtendedQueryTag": {
        "MaxAllowedCount": 128
      },
      "Reindex": {
        "BatchSize": 2,
        "ParallelBatches": 2
      }
    }
  },
  "SqlServer": {
    "ConnectionString": "server=(local);Initial Catalog=Dicom;Integrated Security=true"
  }
}
