{
  "BlobStore": {
    "ConnectionString": "UseDevelopmentStorage=true"
  },
  "ExternalBlobStore": {
    "ConnectionString": "UseDevelopmentStorage=true",
    "ContainerName": "dicomexternalcontainer"
  },
  "DicomFunctions": {
    "DurableTask": {
      "ConnectionName": "DicomFunctions:DurableTask:ConnectionString",
      "ConnectionString": "UseDevelopmentStorage=true",
      "Monitor": {
        "Enabled": false
      }
    },
    "Indexing": {
      "Batching": {
        "MaxParallelCount": 1
      }
    }
  },
  "Logging": {
    "LogLevel": {
      "Default": "Debug",
      "System": "Information",
      "Microsoft": "Information"
    }
  },
  "SqlServer": {
    "ConnectionString": "server=(local);Initial Catalog=Dicom;Integrated Security=true;TrustServerCertificate=true"
  }
}
